    "mime-types": "^2.1.35",
    "probe-image-size": "^7.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.4",
    "string-strip-html": "8.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
