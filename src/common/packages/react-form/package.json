{
  "name": "@zougui/common.react-form",
  "version": "0.1.0",
  "description": "",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "typings": "lib/cjs/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage",
    "compile": "concurrently -n cjs,esm \"npm:compile:cjs\" \"npm:compile:esm\"",
    "compile:watch": "concurrently -n cjs,esm \"npm:compile:cjs:watch\" \"npm:compile:esm:watch\"",
    "compile:cjs": "tsc --module commonjs --outDir lib/cjs --project .",
    "compile:cjs:watch": "npm run compile:cjs -- --watch",
    "compile:esm": "tsc --module esnext --outDir lib/esm --project .",
    "compile:esm:watch": "npm run compile:esm -- --watch",
    "clean": "rimraf -rf ./lib & rimraf -f tsconfig.tsbuildinfo",
    "build": "npm run clean && npm run compile",
    "prepublishOnly": "npm run build && npm run test:coverage"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.4.1",
    "@mui/x-date-pickers": "5.0.0-alpha.5",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@zougui/common.jest-config": "workspace:^0.1.3",
    "concurrently": "^7.2.1",
    "jest": "^27.5.1",
    "jest-date-mock": "^1.0.8",
    "jest-extended": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "react": "^18.0.0"
  },
  "keywords": [],
  "repository": "https://github.com/zougui1/z-monorepo/tree/main/templates/react-package",
  "author": "Zougui",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^2.8.10",
    "@zougui/common.react-store": "workspace:^0.2.0",
    "@zougui/common.type-utils": "workspace:^0.1.0",
    "@zougui/common.yup-utils": "workspace:^0.3.0",
    "luxon": "^2.4.0",
    "react-hook-form": "^7.31.3",
    "yup": "^0.32.11"
  }
}
